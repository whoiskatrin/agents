{
  "name": "@cloudflare/agents-repo",
  "description": "The monorepo for Cloudflare Agents and related work",
  "type": "module",
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "agents",
    "durable objects",
    "workflows"
  ],
  "author": "Sunil Pai <spai@cloudflare.com>",
  "private": true,
  "scripts": {
    "build": "npm run build -w agents -w hono-agents",
    "format": "prettier --write .",
    "typecheck": "npx tsx scripts/typecheck.ts",
    "check": "sherif && prettier --check . && biome lint && npm run typecheck",
    "test": "npm run test -w agents",
    "test:react": "npm run test:react -w agents"
  },
  "license": "MIT",
  "version": "0.0.0",
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.2.10",
    "@ai-sdk/google": "^1.2.14",
    "@ai-sdk/openai": "^1.3.21",
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.2",
    "@cloudflare/vite-plugin": "1.0.13",
    "@cloudflare/vitest-pool-workers": "^0.8.23",
    "@cloudflare/workers-types": "^4.20250430.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.2",
    "@vitest/runner": "^3.1.2",
    "ai": "^4.3.12",
    "autoevals": "^0.0.127",
    "concurrently": "^9.1.2",
    "evalite": "^0.11.3",
    "fast-glob": "^3.3.3",
    "partyserver": "^0.0.68",
    "partysocket": "1.1.3",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sherif": "^1.5.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vitest": "3.1.2",
    "vitest-browser-react": "^0.1.1",
    "workers-ai-provider": "^0.3.0",
    "wrangler": "^4.13.2",
